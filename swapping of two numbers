#include<stdio.h>
int main() {
  double first, second, temp;
  printf("Enter first number: ");
  scanf("%lf", &first);
  printf("Enter second number: ");
  scanf("%lf", &second);
  temp = first;
  first=second;
  second = temp;
  printf("\nAfter swapping, first number = \n", first);
  printf("\nAfter swapping, second number = \n", second);
  return 0;
}
